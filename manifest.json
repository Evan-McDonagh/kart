{
    "name": "Kart",
    "version": "0.0.1",
    "description": "Kart is an extension for playing and looping Mario Kart music.",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
        "scripts": ["src/howler.js","src/background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "manifest_version": 2
}